{
	"name": "@xstyled-theme/utils",
	"version": "0.3.0",
	"description": "Utilities used by xstyled-theme themes to provide a styled-components function to theme consumers",
	"homepage": "/",
	"license": "MIT",
	"author": "La Javaness <dev@lajavaness.com>",
	"main": "build/index.js",
	"files": {
		"0": "build"
	},
	"scripts": {
		"build": "node scripts/build.js",
		"build:ci": "cross-env CI=true yarn build:prod",
		"build:next": "cross-env LJN_ENV=next yarn build",
		"build:prod": "cross-env LJN_ENV=production yarn build",
		"build:staging": "cross-env LJN_ENV=staging yarn build",
		"lint": "yarn eslint --ext .js,.jsx,.ts,.tsx src",
		"lint:fix": "yarn lint --fix",
		"prepublishOnly": "yarn test:ci && yarn build:ci",
		"prettier": "scripts/precommitHookPrettier.sh",
		"prettier:check": "npx prettier \"*/**/*.{js,jsx,ts,tsx,json,rc}\" --ignore-path ./.prettierignore --check",
		"prettier:fix": "npx prettier \"*/**/*.{js,jsx,ts,tsx,json,rc}\" --ignore-path ./.prettierignore --write",
		"test": "node scripts/test.js -u",
		"test:ci": "cross-env CI=true yarn test -b --watchAll=false --verbose --bail"
	},
